    "version": "0.2.0",
    "configurations": [
        {
            "name": "encointer-wallet-flutter",
            "request": "launch",
            "type": "dart",
            "program": "app/lib/main.dart",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/main.dart"
            ],
        },
        {
            "name": "encointer-wallet-flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "app/lib/main.dart",
            "flutterMode": "profile",
        },
        {
            "name": "encointer-wallet-flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "program": "app/lib/main.dart",
            "flutterMode": "release",
        }
    ]
}